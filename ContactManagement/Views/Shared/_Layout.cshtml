@{
    var urlHelper = new UrlHelper(Html.ViewContext.RequestContext);
    var baseurl = urlHelper.Content("~");
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="~/Content/themes/adminlte/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style>
        .modalFix {
            left: 0% !important;
        }
    </style>

    <script src="~/Content/ckeditor/ckeditor.js"></script>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.5 -->
    <link href="~/Content/themes/adminlte/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!-- Font Awesome-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons-->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href="~/Content/themes/adminlte/plugins/datatables/dataTables.bootstrap.css" rel="stylesheet" />

    <!-- Theme style -->
    <link href="~/Content/themes/adminlte/css/AdminLTE.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/themes/adminlte/css/skins/_all-skins.min.css" rel="stylesheet" />

    <!-- iCheck -->
    <link href="~/Content/themes/adminlte/plugins/iCheck/flat/blue.css" rel="stylesheet" />
    <!-- jvectormap -->
    <link href="~/Content/themes/adminlte/plugins/jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    <!-- Date Picker -->
    <link href="~/Content/themes/adminlte/plugins/datepicker/datepicker3.css" rel="stylesheet" />

    <link href="~/Content/themes/adminlte/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet">

    <script src="~/Content/themes/adminlte/plugins/slimScroll/jquery.slimscroll.min.js"></script>

    <!-- Daterange picker -->
    <link href="~/Content/themes/adminlte/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" />
    <!-- bootstrap wysihtml5 - text editor -->
    <link href="~/Content/themes/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/Content/select2.min.css" type="text/css" rel="stylesheet" />
    <script type="text/javascript" src="~/Scripts/select2.full.js"></script>
    <script type="text/javascript" src="~/Scripts/CustomScript.js"></script>
</head>
<body class="sidebar-mini skin-blue">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index", "Home")" class="logo" style="background:#FFFFFF">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <img src="~/Images/011.jpg" style="margin-left:-15px; width:240px; height:55px;" />
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><img src='@Url.Action("ShowPhoto", "Profile", new { User_Name = (string)Session["User_Name"] } )' width="200" class="user-image" alt="User Image"></a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src='@Url.Action("ShowPhoto", "Profile", new { User_Name = (string)Session["User_Name"] } )' width="200" class="img-circle" alt="User Image">

                                    <p>
                                        <small>@DateTime.Today.ToString("dd dddd MMMM - yyyy")</small>
                                    </p>
                                </li>
                                <!-- Menu Body
                                <li class="user-body">
                                    <div class="row">
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Followers</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Sales</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Friends</a>
                                        </div>
                                    </div>
                                </li>-->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="~/Profile" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="~/Users/LogOff" class="btn btn-default btn-flat">Log Off</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">              
                @if (HttpContext.Current.Session["email"] != null && HttpContext.Current.Session["email"].ToString() != "")
                {
                    <ul class="sidebar-menu">
                        <li class="header">MAIN NAVIGATION</li>

                        <li>
                            <a href="@Url.Action("Index", "User")">
                                <i class="fa fa-comment-o"></i> <span>Profile</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Category")">
                                <i class="fa fa-location-arrow"></i> <span>Category</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Contact")">
                                <i class="fa fa-location-arrow"></i> <span>Contact</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("ImportExcel", "Contact")">
                                <i class="fa fa-comment-o"></i> <span>Import Contact</span>
                            </a>
                        </li>                       
                        <li>
                            <a href="@Url.Action("LogOff", "Home")">
                                <i class="fa fa-sign-out"></i> <span>Log Off</span>
                            </a>
                        </li>
                    </ul>
                }
            
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        @RenderSection("featured", required: false)
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->

            <section class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box box-solid">
                            <div class="box-body">
                                @{
                                    var FindMenu = "";
                                 
                                }
                                <ol class="breadcrumb">
                                    <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>

                                   

                                </ol>
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </section>

        </div><!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2017</strong>
        </footer>

    </div><!-- ./wrapper -->
    <!-- jQuery 2.1.4-->
    <!-- jQuery UI 1.11.4-->
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.5 -->
    <script src="~/Content/themes/adminlte/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/themes/adminlte/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/themes/adminlte/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <!-- Morris.js charts
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>-->
    <!-- Sparkline -->
    <script src="~/Content/themes/adminlte/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Content/themes/adminlte/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/themes/adminlte/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/themes/adminlte/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.2/moment.min.js"></script>-->
    <script src="~/Content/themes/adminlte/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Content/themes/adminlte/plugins/datepicker/bootstrap-datepicker.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Content/themes/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/themes/adminlte/plugins/slimScroll/jquery.slimscroll.min.js"></script>

    <!-- FastClick -->
    <script src="~/Content/themes/adminlte/plugins/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/themes/adminlte/js/app.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <!-- AdminLTE for demo purposes -->
    @RenderSection("scripts", required: false)
    <script>
        $(function () { // will trigger when the document is ready
            $('.datepicker').datepicker({
                format: "yyyy-mm-dd"
            }).on('changeDate', function (e) {
                $(this).datepicker('hide');
            }); //Initialise any date pickers
        });

        $(function () {
            //$('.datepicker').datepicker({
            //    format: "yyyy-mm-dd",
            //}).on('changeDate', function (e) {
            //    $(this).datepicker('hide');
            //});
            //$('.daterange').daterangepicker();

            $(".datatable").DataTable({
                "paging": true,
                "lengthChange": true,
                "searching": true,
                "ordering": true,
                "info": true,
                "autoWidth": true,
                "lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]]
            });

            $(".treeview-menu li.active").parent().parent().parent().parent().addClass("active");
            $(".treeview-menu li.active").parent().parent().addClass("active");

        });
    </script>
    <link href="~/Scripts/ValidationEngine.css" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="~/Scripts/jquery.validationEngine-en.js" charset="utf-8"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validationEngine.js" charset="utf-8"></script>
    <script type="text/javascript" src="~/Scripts/myscript.js" charset="utf-8"></script>
    <link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/themes/adminlte/plugins/timepicker/bootstrap-timepicker.min.js"></script>
    <script src="~/Content/themes/adminlte/plugins/daterangepicker/daterangepicker.js"></script>
    <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/cupertino/jquery-ui.css" rel="Stylesheet" type="text/css" />
    @*<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>*@

    <!-- iCheck -->
    <link href="~/Content/themes/adminlte/plugins/iCheck/minimal/_all.css" rel="stylesheet" />
    <script src="~/Content/themes/adminlte/plugins/iCheck/icheck.min.js"></script>
    <script>


        $(function () {

            if (window.history && window.history.pushState) {
                window.history.pushState('forward', null);
                $(window).on('popstate', function () {
                    location.reload();
                });
            }

            //iCheck for checkbox and radio inputs
            $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
                checkboxClass: 'icheckbox_minimal-blue',
                radioClass: 'iradio_minimal-blue'
            });
            //Red color scheme for iCheck
            $('input[type="checkbox"].minimal-red, input[type="radio"].minimal-red').iCheck({
                checkboxClass: 'icheckbox_minimal-red',
                radioClass: 'iradio_minimal-red'
            });
            //Flat red color scheme for iCheck
            $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
                checkboxClass: 'icheckbox_flat-green',
                radioClass: 'iradio_flat-green'
            });
        });
    </script>
</body>
</html>